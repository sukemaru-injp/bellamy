#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules/@turbo/gen/dist/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules/@turbo/gen/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules/@turbo/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules/@turbo/gen/dist/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules/@turbo/gen/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules/@turbo/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.15.3_typescript@5.8.2/node_modules:/Users/ryosukekubota/develop/bellamy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@turbo/gen/dist/cli.js" "$@"
else
  exec node  "$basedir/../@turbo/gen/dist/cli.js" "$@"
fi
